const int trig = 9;
const int Echo = 10;
void setup() {
  pinMode(trig, OUTPUT);
  pinMode(Echo, INPUT);
  Serial.begin(9600);
 
}


// the loop function runs over and over again forever
void loop() {
  digitalWrite(trig, LOW);
  delayMicroseconds(2);
  digitalWrite(trig, HIGH);
  delayMicroseconds(10);
  digitalWrite(trig, LOW);

  float duration = pulseIn(echo, HIGH);
  float distance = (duration * 0.0343) / 2;

  Serial.print("Distance: ");
  Serial.print(distance);
  Serial.println(" cm");
